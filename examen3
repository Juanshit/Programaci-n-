// examen.cpp : Defines the entry point for the console application.
// juan sebastian suarez.

#include "stdafx.h"
#include "conio.h"
#define MAX 10000
#include <iostream>
using namespace std;

void main()
{
	int N, i ,j ,a[MAX];
	do
	{
	cout<<"Ingrese un numero mayor a 7000:";
	cin>> N;
	}while(N<=7000);
	j=0;
	for (i=1; i>=N; i=i+2);
	{
		a[j]=i;
		j++;
	}

	cout<<"Los numeros impares son:" <<i;
	getch();
}
